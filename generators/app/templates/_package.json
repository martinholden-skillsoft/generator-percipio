{
  "name": "<%= options.packagename %>",
  "version": "0.0.0",
  "description": "Call a Percipio API",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/<%= options.name %>/<%= options.packagename %>.git"
  },
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./app.js",
    "lint": "eslint **/*.js",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags && npm run changelog && git push ",
    "postinstall": "",
    "changelog": "rimraf CHANGELOG.md && auto-changelog --template keepachangelog && git add . && git commit . -m changelog && git push"
  },
  "keywords": [
    "skillsoft",
    "percipio"
  ],
  "author": "<%= options.email %>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "global-tunnel-ng": "^2.7.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "pkginfo": "^0.4.1",
    "promise-retry": "^2.0.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "auto-changelog": "^2.2.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2"
  }
}