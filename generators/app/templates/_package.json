{
  "name": "<%= options.packagename %>",
  "version": "0.0.0",
  "description": "Call <%= options.percipioServicename %> API using Axios. Function: <%= options.percipioServicePath %>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/<%= options.name %>/<%= options.packagename %>.git"
  },
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./app.js",
    "lint": "eslint **/*.js",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags && npm run changelog && git push ",
    "postinstall": "",
    "changelog": "rimraf CHANGELOG.md && auto-changelog --template keepachangelog && git add . && git commit . -m changelog && git push"
  },
  "keywords": [
    "skillsoft",
    "percipio"
  ],
  "author": "<%= options.author %>",
  "license": "MIT",
  "dependencies": {
    "access-safe": "^0.1.0",
    "axios": "^0.21.1",
    "axios-rate-limit": "^1.3.0",
    "config": "^3.3.6",
<% if (options.percipioServiceIsPaged) { _%>
    "JSONStream": "^1.3.5",
<% } _%>
    "dotenv-safe": "^8.2.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "retry-axios": "^2.4.0",
    "stream-combiner": "^0.2.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-null": "^2.0.0"
  },
  "devDependencies": {
    "auto-changelog": "^2.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2"
  }
}